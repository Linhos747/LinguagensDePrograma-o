# Dicionário de produtos
produtos = {
    1: ("Notebook", 2500.00),
    2: ("Smartphone", 1200.00),
    3: ("Tablet", 800.00),
    4: ("Fone de Ouvido", 150.00),
    5: ("Teclado", 100.00)
}

# Solicita um código ao usuário
codigo = int(input("Digite o código do produto: "))

# Busca o produto no dicionário
if codigo in produtos:
    nome, preco = produtos[codigo]
    print(f"Produto: {nome}, Preço: R$ {preco:.2f}")
else:
    print("Código de produto não encontrado.")
